{
  "name": "weflex-ui",
  "version": "2.7.3",
  "description": "The UI Component that providing basic form controls which is compatible with React.js",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run lib",
    "test": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/jasmine",
    "css": "node ./build-less.js > lib/framework.css",
    "lib": "babel src -d lib",
    "prebuild": "mkdir -p lib",
    "build": "npm run css && npm run lib",
    "prepublish": "npm run lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/weflex/weflex-ui.git"
  },
  "keywords": [
    "react",
    "form",
    "ui"
  ],
  "author": "Yorkie Liu <yorkiefixer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weflex/weflex-ui/issues"
  },
  "homepage": "https://github.com/weflex/weflex-ui#readme",
  "dependencies": {
    "classnames": "2.2.3",
    "css-animation": "1.2.1",
    "less": "2.6.1",
    "rc-animate": "2.0.3",
    "rc-notification": "1.3.2",
    "rc-progress": "1.0.4",
    "rc-select": "5.10.4",
    "rc-upload": "1.9.0",
    "rc-util": "3.1.3",
    "react-binding-component": "1.0.3",
    "warning": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "cheerio": "0.20.0",
    "istanbul": "^0.4.3",
    "jasmine": "2.4.1",
    "less-loader": "2.2.3",
    "react": "0.14.7",
    "react-binding-component": "1.0.3",
    "react-dom": "0.14.7",
    "style-parser": "^1.1.0"
  }
}
